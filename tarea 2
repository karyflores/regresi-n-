#la dimension que pide la matrix no la genera R por el numero de datos son 21 
#la matriz nececita 20 datos 
#por lo tanto se corrigió el vector de nombre vector 
vector<-11:31
vector<-11:30
View(vector)
dim(vector)<-c(5,4)
length(vector)
rownames(vector)<-c("toluca", "pachuca", "gdl", "mtv")
colnames(vector)<-c("escuela1", "escuela2","escuela3","escuela4","escuela5")
vector[1,3]
##escuela 3 de Toluca
vector[3,5] ## escuela 5 de gld
vector[2,1] #escuela 1 de pachuca
###crear una matriz con la función matriz
función<-matrix(0:17,nrow=6,ncol=3,byrow=T)
View(función)
función[3,3]
